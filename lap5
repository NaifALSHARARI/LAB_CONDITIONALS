movie = "pk"
rate = int(input("inter a score out of 5 "))
world_score = float(input("world_rate:")) 
if rate >= 4 and world_score >= 80:
   print("Highly recommended")
elif rate >= 3 and world_score >= 70:
    print("I recommended it . It is good")
elif rate >= 2 and world_score >= 60:
    print("You should check it out!")
elif rate >= 2 and world_score <= 50: 
    print ("Don't watch it. It is a waste of time")